@model VendaDefinitiva.Models.Produto
@{
}
<p></p>
@{
    ViewData["Title"] = "Entrada";
}
<h2>@ViewData["Title"]</h2>

<h4> Produto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Entrada">

            <div class="form-group">
                <label asp-for="NomeProduto" class="control-label"></label>
                <input asp-for="NomeProduto" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="UnidadeProduto" class="control-label"></label>
                <input asp-for="UnidadeProduto" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="DepartamentoId" class="control-label"></label>
                <input asp-for="DepartamentoId" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Dar Entrada" class="btn btn-default" />
            </div>
        </form>

    </div>
</div>

